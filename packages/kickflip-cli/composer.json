{
    "name": "mallardduck/kickflip-cli",
    "description": "A Laravel Zero based static site generator CLI tool.",
    "keywords": ["ssg", "static site generator", "laravel zero", "cli"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/mallardduck/kickflip-cli/issues",
        "source": "https://github.com/mallardduck/kickflip-cli"
    },
    "authors": [
        {
            "name": "Dan Pock",
            "email": "self@danpock.me"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/log": "^8.0",
        "illuminate/view": "^8.0",
        "laravel-zero/framework": "^8.8",
        "league/commonmark": "^2.0",
        "mallardduck/laravel-traits": "dev-main",
        "spatie/enum": "^3.9",
        "spatie/laravel-markdown": "^2.0",
        "symfony/yaml": "^5.3",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Kickflip\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "XDEBUG_MODE=coverage vendor/bin/pest --coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/kickflip"
    ]
}
